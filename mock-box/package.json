{
  "name": "mock-box",
  "productName": "MockBOX",
  "version": "1.0.0",
  "description": "fake BOX server...  used for dev purposes",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electrontest --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"MockBOX\"",
    "package-linux": "electron-packager . electrontest --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PedalSwitcher/mock-box.git"
  },
  "author": "Ben Turcotte",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PedalSwitcher/mock-box/issues"
  },
  "homepage": "https://github.com/PedalSwitcher/mock-box#readme",
  "devDependencies": {
    "electron": "^8.2.0",
    "electron-builder": "^21.1.5",
    "electron-packager": "^14.2.1"
  },
  "dependencies": {}
}
